{
  "version": 8,
  "sources": {
    "custom-tiles": {
      "type": "vector",
      "tiles": ["http://10.0.2.2:8080/data/{z}/{x}/{y}.pbf"],
      "minzoom": 5,
      "maxzoom": 14
    }
  },
  "layers": [
    {
      "id": "buildings",
      "type": "line",
      "source": "custom-tiles",
      "source-layer": "building",
      "paint": { "line-color": "#198EC8" }
    },
    {
      "id": "land",
      "type": "fill",
      "source": "custom-tiles",
      "source-layer": "landcover",
      "paint": {
        "fill-color": "#3388ff"
      }
    }
  ]
}
